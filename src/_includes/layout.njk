                            .cookie
                            .split(";"),
                        consentCookie = "",
                        consentDate = Cookiebot.consentUTC,
                        consentExpiry = new Date(consentDate.setMonth(consentDate.getMonth() + 12)),
                        now = new Date();

                    for (var i = 0; i < cookies.length; i++) {
                        if (cookies[i].indexOf("CookieConsent") !== -1) {
